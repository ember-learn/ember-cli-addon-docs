<div class="docs-demo-example">
  {{#if error}}
    <p>{{error}}</p>
  {{else if compiledTemplateA}}
    {{docs-demo/x-live-example-demo compiledTemplate=compiledTemplateA}}
  {{else if compiledTemplateB}}
    {{docs-demo/x-live-example-demo compiledTemplate=compiledTemplateB}}
  {{/if}}
</div>

{{ember-ace value=rawTemplate
  class='docs-demo__live-editor'
  mode='ace/mode/handlebars'
  showPrintMargin=false
  showGutter=false
  highlightActiveLine=false
  minLines=5
  maxLines=20
  update=(action 'recompile')}}
