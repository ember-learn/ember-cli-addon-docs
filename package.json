{
  "name": "ember-cli-addon-docs",
  "version": "5.0.0",
  "description": "Easy, beautiful docs for your OSS Ember addons",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "repository": "https://github.com/ember-learn/ember-cli-addon-docs",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "deploy": "ember deploy production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "prepare": "./scripts/link-them.sh",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test --test-port=0",
    "test:node": "mocha tests-node --recursive",
    "test:test-apps": "cd test-apps/new-addon && yarn test"
  },
  "dependencies": {
    "@csstools/postcss-sass": "^5.0.0",
    "@ember/render-modifiers": "^2.0.0",
    "@ember/test-waiters": "^3.0.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/syntax": "^0.84.0",
    "@glimmer/tracking": "^1.0.4",
    "@handlebars/parser": "^2.1.0",
    "broccoli-bridge": "^1.0.0",
    "broccoli-caching-writer": "^3.0.3",
    "broccoli-filter": "^1.3.0",
    "broccoli-funnel": "^3.0.8",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-persistent-filter": "^3.1.0",
    "broccoli-plugin": "1.5.1 - 5",
    "broccoli-source": "^3.0.0",
    "broccoli-stew": "^3.0.0",
    "chalk": "4.1.2",
    "ember-auto-import": "^2.4.0",
    "ember-cli-autoprefixer": "^2.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-clipboard": "^0.16.0",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-postcss": "^8.0.0",
    "ember-cli-string-helpers": "^6.1.0",
    "ember-cli-string-utils": "^1.1.0",
    "ember-cli-version-checker": "^5.1.0",
    "ember-code-snippet": "^3.0.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^2.0.0",
    "ember-fetch": "^8.1.1",
    "ember-keyboard": "^8.0.0",
    "ember-modal-dialog": "^4.0.0",
    "ember-responsive": "^5.0.0",
    "ember-router-generator": "^2.0.0",
    "ember-router-scroll": "^4.1.2",
    "ember-set-helper": "^2.0.1",
    "ember-svg-jar": "^2.3.4",
    "ember-tether": "^2.0.1",
    "ember-truth-helpers": "^3.0.0",
    "esm": "^3.2.25",
    "execa": "5.1.1",
    "fs-extra": "^10.0.0",
    "git-repo-info": "^2.1.1",
    "highlight.js": "^11.4.0",
    "hosted-git-info": "^5.0.0",
    "html-entities": "^2.3.2",
    "jsdom": "^20.0.0",
    "json-api-serializer": "^2.6.6",
    "line-column": "^1.0.2",
    "lodash": "^4.17.15",
    "lunr": "^2.3.7",
    "marked": "^4.0.12",
    "node-html-parser": "^5.2.0",
    "pad-start": "^1.0.2",
    "parse-git-config": "^3.0.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.0",
    "quick-temp": "^0.1.8",
    "semver": "^7.3.2",
    "striptags": "^3.1.1",
    "tailwindcss": "1.9.6",
    "tracked-toolbox": "^2.0.0",
    "walk-sync": "^3.0.0",
    "yuidocjs": "^0.10.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.4",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^1.2.0",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "common-tags": "^1.8.2",
    "ember-classy-page-object": "rwwagner90/ember-classy-page-object#1a126eaf9077587fa85bfe9fd2c6d3cc6d1f7f1b",
    "ember-cli": "~4.1.1",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-blueprint-test-helpers": "^0.19.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-mirage": "^3.0.0-alpha.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.8.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.6.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.0.0",
    "ember-test-selectors": "^6.0.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "miragejs": "^0.1.43",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.12.1",
    "release-it-lerna-changelog": "^4.0.1",
    "webpack": "^5.68.0"
  },
  "peerDependencies": {
    "ember-data": ">= 3.0.0",
    "ember-fetch": "^8.1.1"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-htmlbars",
      "ember-composable-helpers",
      "ember-svg-jar",
      "ember-code-snippet"
    ],
    "after": [
      "ember-modal-dialog"
    ]
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
